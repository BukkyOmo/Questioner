{
  "name": "questioner",
  "version": "1.0.0",
  "description": "Questioner is an app that crowd-sources questions for meetups/events.",
  "main": "index.js",
  "engines": {
    "node": "10.13.x"
  },
  "scripts": {
    "start": "gulp serve",
    "clean": "gulp clean",
    "seed": "babel-node ./app/v1/Model/seeder.js",
    "pretest": "cross-env NODE_ENV=test npm run migration",
    "test": "cross-env NODE_ENV=test npm run seed && cross-env NODE_ENV=test nyc mocha --exit --compilers js:babel-core/register --timeout 30000 ./app/test/**/*.js",
    "migration": "babel-node ./app/v1/Model/db",
    "migrations:up": "db-migrate up",
    "migrations:down": "db-migrate down -c 50",
    "migrate:up": "db-migrate up --config config/database.json -e test",
    "migrate:down": "db-migrate down -c 50 --config config/database.json -e test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BukkyOmo/Questioner.git"
  },
  "author": "Bukola Omosefunmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BukkyOmo/Questioner/issues"
  },
  "homepage": "https://github.com/BukkyOmo/Questioner#readme",
  "devDependencies": {
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "expect": "^24.1.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.7",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.0.0",
    "del": "^2.2.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "file-stream-rotator": "^0.5.7",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-exit": "0.0.2",
    "gulp-istanbul": "^1.1.1",
    "gulp-jshint-jenkins-reporter": "^1.1.0",
    "gulp-load-plugins": "^1.4.0",
    "gulp-mocha": "^3.0.1",
    "gulp-newer": "^1.3.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-task-listing": "^1.0.1",
    "gulp-util": "^3.0.8",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "node-cmd": "^3.0.0",
    "password-hash": "^1.2.2",
    "pg": "^7.18.2",
    "run-sequence": "^2.2.1",
    "swagger-ui-express": "^4.0.6",
    "yamljs": "^0.3.0"
  },
  "env": {
    "NODE_ENV": "development",
    "PORT": 5500
  },
  "directories": {
    "test": "test"
  }
}
